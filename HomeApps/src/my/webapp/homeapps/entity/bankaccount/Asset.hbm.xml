<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="my.webapp.homeapps.entity.bankaccount">
	<class name="Asset" table="ASSET">
		<id name="id" column="id"><generator class="native" /></id>
		<timestamp name="lastUpdTs" column="LAST_UPD_TS" />
		<property name="deleteMark" column="DELETE_MARK" />
		<property name="lastUpdUr" column="LAST_UPD_UR" />

		<property name="name" column="NAME" />
		<property name="category" column="CATEGORY" />
		<property name="discontinued" column="DISCONTINUED" />
		
		<set name="values" inverse="true" lazy="false" >
			<key column="ASSET_ID" />
			<one-to-many class="AssetValue" />
		</set>

		<filter name="hideDeleted" />
	</class>
</hibernate-mapping>
